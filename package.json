	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fullstackcaveman/star-wars-universe/issues"
	},
	"homepage": "https://github.com/fullstackcaveman/star-wars-universe#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"helmet": "^4.4.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.12.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"node": "^15.10.0"
